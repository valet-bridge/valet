set dd = liga2010

perl names2.pl ${dd}/butler?.html | sort | uniq > names.txt

perl number.pl names.txt > n.txt
mv n.txt names.txt

perl pairs2.pl ${dd}/butler?.html | sort | uniq > pairs.txt

perl parse.pl names.txt pairs.txt ${dd}/* > scores.txt

perl parse.pl names.txt pairs.txt ${dd}/k1* > scores1.txt
perl parse.pl names.txt pairs.txt ${dd}/k2* > scores2a.txt
perl parse.pl names.txt pairs.txt ${dd}/k3* > scores2b.txt
