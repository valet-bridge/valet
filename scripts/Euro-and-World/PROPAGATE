echo 2009-Open-Euro
cat 2009-Open-Euro/Open/RR/*/scores.txt | sort -n > 2009-Open-Euro/Open/RR/scores.txt
cat 2009-Open-Euro/Open/Swiss/*/scores.txt | sort -n > 2009-Open-Euro/Open/Swiss/scores.txt
perl mergemap.pl 2009-Open-Euro/Open
cat 2009-Open-Euro/Women/*/scores.txt | sort -n > 2009-Open-Euro/Women/scores.txt
cat 2009-Open-Euro/Seniors/*/scores.txt | sort -n > 2009-Open-Euro/Seniors/scores.txt
cat 2009-Open-Euro/Mixed/RR/*/scores.txt | sort -n > 2009-Open-Euro/Mixed/RR/scores.txt
cat 2009-Open-Euro/Mixed/Swiss/*/scores.txt | sort -n > 2009-Open-Euro/Mixed/Swiss/scores.txt
perl mergemap.pl 2009-Open-Euro/Mixed
perl mergemap.pl 2009-Open-Euro

echo 2009-World
perl mergemap.pl 2009-World

