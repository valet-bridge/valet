echo 2005-World
perl mergemap.pl 2005-World

echo 2006-Euro
perl mergemap.pl 2006-Euro/Seniors/Final
perl mergemap.pl 2006-Euro/Seniors
perl mergemap.pl 2006-Euro

echo 2007-World
perl mergemap.pl 2007-World

echo 2007-WTNT
cp 2007-WTNT/Open/scores.txt 2007-WTNT/scores.txt

echo 2007-Open-Euro
perl mergemap.pl 2007-Open-Euro/Open/RR
perl mergemap.pl 2007-Open-Euro/Open/Swiss
perl mergemap.pl 2007-Open-Euro/Open
perl mergemap.pl 2007-Open-Euro/Women/RR
cp 2007-Open-Euro/Women/RR/scores.txt 2007-Open-Euro/Women
perl mergemap.pl 2007-Open-Euro/Seniors/RR
cp 2007-Open-Euro/Seniors/RR/scores.txt 2007-Open-Euro/Seniors
perl mergemap.pl 2007-Open-Euro/Mixed/RR
perl mergemap.pl 2007-Open-Euro/Mixed/Swiss
perl mergemap.pl 2007-Open-Euro/Mixed
perl mergemap.pl 2007-Open-Euro

